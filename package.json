{
  "name": "@elevio/kb-javascript-starter",
  "version": "0.1.0",
  "description": "This is a starter kit for Elevios KB builder project",
  "main": "src/index.js",
  "scripts": {
    "start": "elevio-kb dev",
    "extract-locales": "elevio-kb extract-locales",
    "start:compiled": "USE_COMPILED_FILES=true elevio-kb dev",
    "build": "elevio-kb build",
    "build:analyse": "elevio-kb build-analyse",
    "save": "elevio-kb save-version",
    "deploy": "elevio-kb publish-version",
    "prettier:js": "prettier --check kb-javascript-starter --write",
    "build:js": "babel src --out-dir kb-javascript-starter --extensions \".ts,.tsx\"",
    "copy:js": "./build-tools/copyJs.sh",
    "kb-js": "npm run build:js && npm run prettier:js && npm run copy:js"
  },
  "repository": "https://github.com/dixahq/kb-javascript-starter",
  "author": "support@elev.io",
  "dependencies": {
    "@elevio/kb-kit": "^0.0.70",
    "classnames": "^2.3.1",
    "prop-types": "^15.7.2",
    "react": "^17.0.1",
    "react-dom": "^17.0.1"
  },
  "license": "MIT",
  "private": true,
  "devDependencies": {
    "@babel/cli": "^7.13.14",
    "@babel/core": "^7.13.14",
    "@babel/plugin-proposal-class-properties": "^7.13.0",
    "@babel/preset-env": "^7.13.12",
    "@babel/preset-react": "^7.13.13",
    "@babel/preset-typescript": "^7.13.0",
    "prettier": "2.2.1",
    "typescript": "^4.2.3"
  }
}
